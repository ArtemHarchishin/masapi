package ch.capi.net{	/**	 * Represents some simple data type constants for the data of a <code>ILoadableFile</code>	 * 	 * @see			ch.capi.net.ILoadableFile#getData()	ILoadableFile.getData()	 * @author		Cedric Tabin - thecaptain	 * @version		1.0	 */	public class DataType	{		//---------//		//Constants//		//---------//				/**		 * Defines the <code>XML</code> data type.		 */		public static const XML:String = "XML";				/**		 * Defines the <code>XMLDocument</code> data type.		 */		public static const XML_DOCUMENT:String = "flash.xml.XMLDocument";				/**		 * Defines the <code>ByteArray</code> data type.		 */		public static const BYTE_ARRAY:String = "flash.utils.ByteArray";		/**		 * Defines the <code>URLVariables</code> data type.		 */		public static const URL_VARIABLES:String = "flash.net.URLVariables";				/**		 * Defines the <code>StyleSheet</code> data type.		 */		public static const STYLE_SHEET:String = "flash.text.StyleSheet";				/**		 * Defines the <code>Sound</code> data type.		 */		public static const SOUND:String = "flash.media.Sound";				/**		 * Defines the <code>Stream</code> data type.		 */		public static const STREAM:String = "flash.net.URLStream";				/**		 * Defines the <code>Loader</code> data type.		 */		public static const LOADER:String = "flash.display.Loader";				/**		 * Defines the <code>Bitmap</code> data type.		 */		public static const BITMAP:String = "flash.display.Bitmap";				/**		 * Defines the <code>BitmapData</code> data type.		 */		public static const BITMAP_DATA:String = "flash.display.BitmapData";	}}