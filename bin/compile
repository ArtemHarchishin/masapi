#!/bin/sh

source config
cd ..

output_swc=$default_output_swc

if [ $# -eq 1 ]; then
	if [ $1 = "-help" ]; then
		echo "Usage : $0 [output_swc]"
		exit
	else
		output_swc=$1
	fi
fi

$flex_sdk_path/mxmlc -strict -optimize -warnings -compiler.source-path $source_path -output $output_swc ./src/ch/capi/net/CompositeMassLoader.as


